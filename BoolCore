( \(Result : *)
→ \(callback : ./BOOL_CORE Result)
→ callback
    (∀(a:*) → a → a → a)                                --  Bool
    (\(a:*) → \(x:a) → \(y:a) → x)                      --  true
    (\(a:*) → \(x:a) → \(y:a) → y)                      --  false
    (\(x : ∀(b:*) → b → b → b) → x)                     --  if
)
